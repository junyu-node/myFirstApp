
<swiper indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image" />
    </swiper-item>
  </block>
</swiper>
<view>
    <view wx:for="{{news}}" class="newsList">
        <image src="{{item.img}}" class="newsImg"/>
        <text class="newsText">{{item.text}}</text>
    </view>
</view>
